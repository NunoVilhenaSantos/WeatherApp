<ContentPage
    x:Class="WeatherApp.AboutPage"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml">

    <StackLayout>
        <Image
            HeightRequest="100"
            Source="appiconfg_new.svg"
            WidthRequest="100" />
        <Label
            FontAttributes="Bold"
            FontSize="20"
            HorizontalOptions="CenterAndExpand"
            Text="Seu Nome" />
